<view><view><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="bg-gray"><text>借车人信息</text></view><view class="uni-flex uni-row uni-card"><text class="word" style="-webkit-flex:1;flex:1;">姓名</text><input style="-webkit-flex:1;flex:1;" placeholder="请输入..." data-event-opts="{{[['input',[['__set_model',['$0','lendName','$event',[]],['formData']]]]]}}" value="{{formData.lendName}}" bindinput="__e"/></view><view class="uni-flex uni-row uni-card"><text class="word" style="-webkit-flex:1;flex:1;">身份证拍照上传</text><image style="width:60rpx;height:60rpx;margin-top:20rpx;margin-right:110rpx;" src="/static/photo.png" data-event-opts="{{[['tap',[['photo',['$event']]]]]}}" bindtap="__e"></image></view><view class="uni-flex uni-row uni-card"><text class="word" style="-webkit-flex:1;flex:1;">驾驶证拍照上传</text><image style="width:60rpx;height:60rpx;margin-top:20rpx;margin-right:110rpx;" src="/static/photo.png" data-event-opts="{{[['tap',[['photo',['$event']]]]]}}" bindtap="__e"></image></view><view class="uni-flex uni-row uni-card"><text class="word" style="-webkit-flex:1;flex:1;">手机号码</text><input style="-webkit-flex:1;flex:1;" type="number" placeholder="请输入..." data-event-opts="{{[['input',[['__set_model',['$0','lendPhone','$event',[]],['formData']]]]]}}" value="{{formData.lendPhone}}" bindinput="__e"/></view><view class="bg-gray"><text>借车信息</text></view><view class="uni-flex uni-row uni-card"><text class="word" style="-webkit-flex:1;flex:1;">选择车辆</text><picker class="input" style="-webkit-flex:1;flex:1;" range="{{licenseArray}}" range-key="license" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><label>{{licenseArray[index].license}}</label></picker></view><view class="uni-flex uni-row uni-card"><text class="word" style="-webkit-flex:1;flex:1;">借车时间</text><picker mode="date" value="{{lendDate}}" data-event-opts="{{[['change',[['bindLendDateChange',['$event']]]]]}}" bindchange="__e"><view class="uni-inp